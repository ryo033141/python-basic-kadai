      "source": [
        "import random\n",
        "\n",
        " # 1から15の間のランダムな整数\n",
        "var = random.randint(1, 15)\n",
        "print(\"乱数の確認：\" + str(var))\n",
        "\n",
        "# FizzBuzz問題のプログラム\n",
        "if var % 3 == 0 and var % 5 == 0:\n",
        "  print(\"FizzBuzz\")\n",
        "elif var % 3 == 0:\n",
        "  print(\"Fizz\")\n",
        "elif var % 5 == 0:\n",
        "  print(\"Buzz\")\n",
        "else:\n",
        "  print(var)"
      ]
    }
  ]
