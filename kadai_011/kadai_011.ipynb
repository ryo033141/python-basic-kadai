      "source": [
        "# リストの中身を先頭から全て取り出すプログラム\n",
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "# for文を使った場合\n",
        "for array_output in array:\n",
        "  print(array_output)\n",
        "\n",
        "# while文を使った場合\n",
        "\n",
        "# インデックスを初期化\n",
        "i = 0\n",
        "\n",
        "# while文を使ってリストの最後まで繰り返す\n",
        "while i < len(array):\n",
        "    print(array[i])\n",
        "    # インデックスを1増やす\n",
        "    i = i + 1"
      ]
    }
